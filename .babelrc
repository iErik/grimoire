{
  "presets": [
    ["env", { "targets": { "node": 6 }, "useBuiltIns": true }],
    "stage-0",
    "react"
  ],
  "plugins": [
    "add-module-exports",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-es2015-classes"
  ],
  "env": {
    "production": {
      "plugins": [
        "babel-plugin-dev-expression"
      ]
    }
  }
}
